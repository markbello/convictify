<h1>Create New Guard</h1>

<%= form_for @guard do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br/>
  <%= f.label :cell_block_id %>
  <%= f.collection_select :cell_block_id, @cell_blocks, :id, :name %>
  <br/>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit %>

<% end %>
