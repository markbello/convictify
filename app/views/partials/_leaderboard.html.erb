<h1>Leaderboard</h1>


<div class="ui list">
  <% @prisoners.each do |prisoner| %>
    <div class="item">
      <img class="ui avatar image circular" src="http://i.pravatar.cc/50">
      <div class="content">
        <a class="header"><%= link_to prisoner.full_name, prisoner %></a>
        <div class="description"><%= prisoner.nickname %></div>
      </div>
    </div>
  <% end %>
</div>
