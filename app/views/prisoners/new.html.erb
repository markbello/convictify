<h1>Create New Prisoner</h1>

<%= form_for @prisoner do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br/>
  <%= f.label :nickname %>
  <%= f.text_field :nickname %>
  <br/>
  <%= f.label :conviction %>
  <%= f.text_field :conviction %>
  <%= f.label :release_date %>
  <%= f.text_field :release_date %>
  <br/>
  <%= f.label :cell_id %>
  <%= f.text_field :cell_id %>
  <br/>
  <%= f.submit %>
<% end %>
